let c;
let a = 34;
let b = 21;

console.log(a+b) //first expression
// Prediction: 55, it will add a and b
// Actual:

a = 2;

console.log(a+b) //second expression
// Prediction: 23, a=34 will be overwritten by a=2
// Actual:

//1. 55, it add both 34 and 21
//2. 23 since a will be overwritten as 2
//3. c is undefined
//4. the outcome will be 75 (since 3+4 add and concat 5)



/*
Excercise 5
*/

typeof(15)
// Prediction: number 15 is a number typr


typeof(5.5) 
// Prediction:float 5.5 is a number type


typeof(NaN)
// Prediction:number nan is a number type


typeof("hello")
// Prediction:string it is a string type between ""


typeof(true)
//prediction : boolean , it is boolean type


typeof(1 != 2)
// Prediction:boolean, it is a boolean


"hamburger" + "s"
// Prediction: hamburgers concat both string


"hamburgers" - "s"
// Prediction: undefined - is not a valid operator for string beside 
// Actual: nan

"1" + "3"
// Prediction:13 concat both 1 and 3

"1" - "3"
// Prediction:-2 substraction 


"johnny" + 5
// Prediction: johnny5 concat johnny and 5


"johnny" - 5
// Prediction: undefined  - not a valid operator


99 * "hello"
// Prediction: nan since not a valid operator


1 != 1
// Prediction: false 1 is equal to 1


1 == "1"
// Prediction:true both are equal 


1 === "1"
// Prediction:false not equal to "1"




